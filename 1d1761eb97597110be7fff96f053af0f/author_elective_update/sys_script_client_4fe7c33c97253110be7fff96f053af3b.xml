        <condition/>
        <description>Ensures that the 'adjustment for rough gems" field is set correctly when the form loads.</description>
        <field>rough</field>
        <global>true</global>
        <isolate_script>false</isolate_script>
        <messages/>
        <name>Rough Gem Adjustment On Load</name>
        <order/>
        <script><![CDATA[function onLoad() {
    
	// When the form loads, fetch the value of the Rough checkbox.
	var theRough = g_form.getValue('rough');

	// This line of code is for testing purposes.
	// console.log("The value of the Rough checkbox is: " + theRough + "!");

	// Use an If Statement that sets the value of the rough adjustment field
	if( theRough === true) {
		g_form.setValue('adjustment_for_rough_gems', '1');	
	} else {
		g_form.setValue('adjustment_for_rough_gems', '0');

	}
}]]></script>
