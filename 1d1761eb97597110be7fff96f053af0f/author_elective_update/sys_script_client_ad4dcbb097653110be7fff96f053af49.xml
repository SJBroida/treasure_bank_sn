        <messages/>
        <name>Rough Gem Adjustment On Change</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }

	// This line of code is for testing purposes.
	console.log("The value of the Rough checkbox is: " + newValue + "!");

	// Use an If Statement that sets the value of the rough adjustment field
	if(newValue === "true") {
		console.log("The If Statement sees the newValue as true!");
		g_form.setValue('adjustment_for_rough_gems', '0');
	} else {
		console.log("The If Statement sees the newValue as false!");
		g_form.setValue('adjustment_for_rough_gems', '1');
	}

}]]></script>
